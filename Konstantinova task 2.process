      <Content>&lt;ToolboxItemSettings DisplayName="Получить письма через IMAP" InternalName="GetIMAPMailMessages" FriendlyName="Получить письма с использованием протокола IMAP" Width="200" Height="75" PathToImage="ExternalToolboxes\Image\ico_Email.png" PathToToolboxImage="{x:Null}" ImageType="LeftImage" NoDelete="False" Invisible="False" Proportional="True" Version="69" Code="&amp;#xA;  &amp;#xA;function GetIMAPMailMessages{{{block_id}}}&amp;#xA;{     &amp;#xA;    {AddTypesMail}&amp;#xA;    $__Error.Enter(&amp;quot;{{{block_native_id}}}&amp;quot;, &amp;quot;{{{ErrorLevel}}}&amp;quot;,&amp;quot;{{{ErrorDirection}}}&amp;quot;,&amp;quot;{{{ReportLevel}}}&amp;quot;,&amp;quot;{{{ErrorMessage}}}&amp;quot;);&amp;#xA;    $__Error.AddConnectors(&amp;quot;{{{connector-Error}}}&amp;quot;, &amp;quot;{{{connectorRight}}}&amp;quot;, &amp;quot;{{{connectorBottom}}}&amp;quot;);&amp;#xA;        &amp;#xA;    $port        = {{{Port}}};  &amp;#xA;    $Server      = {{{Server}}};&amp;#xA;    $usessl      = ${{{UseSSL}}};&amp;#xA;    $Login       = {{{Login}}};&amp;#xA;    $Password    = {{{Password}}}; &amp;#xA;    $Timeout     = {{{Timeout}}};   &amp;#xA;    $TopCount    = {{{TopCount}}};&amp;#xA;    $DeleteAfter = ${{{DeleteAfter}}};&amp;#xA;    $SetAsRead   = ${{{SetAsRead}}};&amp;#xA;    $OnlyUnread  = ${{{OnlyUnread}}}; &amp;#xA;    $MailFolder    = {{{MailFolder}}};&amp;#xA;        &amp;#xA;    try&amp;#xA;    {&amp;#xA;     &amp;#xA;        if($port.Length -eq 0 -or $port -eq &amp;quot;0&amp;quot;)&amp;#xA;        {&amp;#xA;            if($usessl) {$port = &amp;quot;993&amp;quot;;}&amp;#xA;            else {$port = &amp;quot;143&amp;quot;;}    &amp;#xA;        }&amp;#xA;            &amp;#xA;        $Timeout = $Timeout * 1000;&amp;#xA;        &amp;#xA;        $clsMail = [MailHelpers.ClassMain]::new();&amp;#xA;        $textresult  = $clsMail.GetImapClient($Server, [int]$port, $usessl, $Login, $Password, [int]$Timeout);&amp;#xA;        if($textresult.Length -gt 0)&amp;#xA;        {